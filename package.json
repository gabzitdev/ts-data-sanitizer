{
  "name": "ts-data-sanitizer",
  "version": "1.0.12",
  "description": "A TypeScript utility for sanitizing data by removing empty values, functions, and nulls",
  "main": "dist/ts-data-sanitizer.js",
  "module": "dist/ts-data-sanitizer.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-push": "sh .husky/pre-push"
    }
  },
  "keywords": [
    "typescript",
    "sanitize",
    "data",
    "clean",
    "utility"
  ],
  "author": "gabzitdev",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitest/coverage-v8": "^1.0.0",
    "husky": "^9.1.7",
    "typescript": "^5.0.4",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabzitdev/ts-data-sanitizer.git"
  }
}
